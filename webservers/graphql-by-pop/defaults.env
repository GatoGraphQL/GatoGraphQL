## Environment variables
APPLICATION_ENVIRONMENT=development
APPLICATION_VERSION=0.1.23
# Request
ENABLE_PASSING_STATE_VIA_REQUEST=true
ENABLE_PASSING_ROUTING_STATE_VIA_REQUEST=true
ENABLE_MODIFYING_ENGINE_BEHAVIOR_VIA_REQUEST=true
# Cache
CACHE_CONTAINER_CONFIGURATION=true
ENABLE_COMPONENT_MODEL_CACHE=true
USE_COMPONENT_MODEL_CACHE=true
USE_SCHEMA_DEFINITION_CACHE=true
CONTAINER_CONFIGURATION_CACHE_DIRECTORY=/app/wordpress/wp-content/graphql-by-pop/cache
# App settings
ENABLE_SETTING_NAMESPACING_BY_URL_PARAM=true
ENABLE_SETTING_MUTATION_SCHEME_BY_URL_PARAM=true
ENABLE_ENABLING_GRAPHQL_INTROSPECTION_BY_URL_PARAM=true
ADD_VERSION_TO_GRAPHQL_SCHEMA_FIELD_DESCRIPTION=true
DEFAULT_CACHE_CONTROL_MAX_AGE=3600
ENABLE_SEMANTIC_VERSION_CONSTRAINTS=true
ENABLE_FEEDBACK_CATEGORY_EXTENSIONS=LOG
SEND_TRACES_TO_LOG=true
SITE_NAME=PoP API Demo
USER_MUST_BE_LOGGED_IN_TO_ACCESS_ROLES_FIELDS=true
ENABLE_MULTIPLE_QUERY_EXECUTION=true
ENABLE_COMPOSABLE_DIRECTIVES=true
ADD_GRAPHQL_INTROSPECTION_PERSISTED_QUERY=true
QUERYABLE_CUSTOMPOST_TYPES=post,page
SETTINGS_ENTRIES=home,blogname,blogdescription,siteurl,WPLANG,posts_per_page,comments_per_page,date_format,time_format,blog_charset
CUSTOMPOST_META_ENTRIES=_edit_last
USER_META_ENTRIES=last_name
# REST endpoints: query by sensitive fields (eg: /users/api/rest/?emails=...)
EXPOSE_SENSITIVE_DATA_IN_SCHEMA=true
# Max number of results
CATEGORY_LIST_MAX_LIMIT = 100
CUSTOMPOST_LIST_MAX_LIMIT = 100
MEDIA_LIST_MAX_LIMIT = 100
PAGE_LIST_MAX_LIMIT = 100
POSTCATEGORY_LIST_MAX_LIMIT = 100
POSTTAG_LIST_MAX_LIMIT = 100
POST_LIST_MAX_LIMIT = 100
TAG_LIST_MAX_LIMIT = 100
USER_LIST_MAX_LIMIT = 100