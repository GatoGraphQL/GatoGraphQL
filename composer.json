    "scripts": {
        "all": [
            "@validate-monorepo",
            "@test",
            "@analyse",
            "@fix-style"
        ],
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit --log-junit=phpunit-test-results.xml"
        ],
        "unit-test": "phpunit --filter='/^((?!Integration).)*$/' --log-junit=phpunit-test-results.xml",
        "stopping-unit-test": "phpunit --filter='/^((?!Integration).)*$/' --log-junit=phpunit-test-results.xml --order-by=defects --stop-on-error --stop-on-failure",
        "integration-test": [
            "Composer\\Config::disableProcessTimeout",
            "@optimize-autoloader",
            "phpunit --filter='Integration' --log-junit=phpunit-test-results.xml",
            "@deoptimize-autoloader"
        ],
        "stopping-integration-test": [
            "Composer\\Config::disableProcessTimeout",
            "@optimize-autoloader",
            "phpunit --filter='Integration' --log-junit=phpunit-test-results.xml --order-by=defects --stop-on-error --stop-on-failure",
            "@deoptimize-autoloader"
        ],
        "integration-test-prod": [
            "Composer\\Config::disableProcessTimeout",
            "INTEGRATION_TESTS_WEBSERVER_DOMAIN=gatographql-for-prod.lndo.site phpunit --filter='Integration'"
        ],
        "stopping-integration-test-prod": [
            "Composer\\Config::disableProcessTimeout",
            "INTEGRATION_TESTS_WEBSERVER_DOMAIN=gatographql-for-prod.lndo.site phpunit --filter='Integration' --log-junit=phpunit-test-results.xml --order-by=defects --stop-on-error --stop-on-failure"
        ],
        "debug": "XDEBUG_TRIGGER=1 phpunit",
        "check-style": "php -d memory_limit=512M vendor/bin/phpcs -n src $(monorepo-builder source-packages --config=config/monorepo-builder/monorepo-builder-without-upstream-packages.php --psr4-only --subfolder=src --subfolder=tests)",
        "check-style-wp": "php -d memory_limit=512M vendor/bin/phpcs --standard=phpcs/WordPress-Custom/ruleset.xml -n $(monorepo-builder source-packages --config=config/monorepo-builder/monorepo-builder-without-upstream-packages.php --psr4-only --wp-only --subfolder=src --subfolder=tests)",
        "fix-style": "php -d memory_limit=512M vendor/bin/phpcbf -n src $(monorepo-builder source-packages --config=config/monorepo-builder/monorepo-builder-without-upstream-packages.php --psr4-only --subfolder=src --subfolder=tests)",
        "fix-style-wp": "php -d memory_limit=512M vendor/bin/phpcbf --standard=phpcs/WordPress-Custom/ruleset.xml -n src $(monorepo-builder source-packages --config=config/monorepo-builder/monorepo-builder-without-upstream-packages.php --psr4-only --wp-only --subfolder=src --subfolder=tests)",
        "analyse": "phpstan analyse --memory-limit 1G --ansi",
        "preview-src-downgrade": "rector process $(monorepo-builder source-packages --config=config/monorepo-builder/source-packages.php --psr4-only --subfolder=src) --config=config/rector/downgrade/monorepo/rector.php --ansi --dry-run || true",
        "preview-vendor-downgrade": "ci/downgrade/downgrade_code.sh config/rector/downgrade/monorepo/rector.php --dry-run \"\" \"\" \"\" \"$(monorepo-builder local-package-owners --config=config/monorepo-builder/local-package-owners.php)\" || true",
        "preview-code-downgrade": [
            "@preview-src-downgrade",
            "@preview-vendor-downgrade"
        ],
        "preview-gatographql-downgrade": "bash -x ci/downgrade/downgrade_code.sh config/rector/downgrade/gatographql/rector.php --dry-run layers/GatoGraphQLForWP/plugins/gatographql \"\" \"\" \"$(monorepo-builder local-package-owners --config=config/monorepo-builder/local-package-owners.php)\"",
        "ssh-server": [
            "Composer\\Config::disableProcessTimeout",
            "composer ssh-server -d webservers/gatographql"
        ],
        "ssh-server-prod": "composer ssh-server -d webservers/gatographql-for-prod",
        "build-server": [
            "Composer\\Config::disableProcessTimeout",
            "composer build-server -d webservers/gatographql"
        ],
        "build-server-prod": [
            "Composer\\Config::disableProcessTimeout",
            "composer build-server -d webservers/gatographql-for-prod"
        ],
        "init-server": "composer init-server -d webservers/gatographql",
        "init-server-prod": "composer init-server -d webservers/gatographql-for-prod",
        "start-server": "composer start-server -d webservers/gatographql",
        "start-server-prod": "composer start-server -d webservers/gatographql-for-prod",
        "destroy-server": "composer destroy-server -d webservers/gatographql",
        "destroy-server-prod": "composer destroy-server -d webservers/gatographql-for-prod",
        "reset-db": [
