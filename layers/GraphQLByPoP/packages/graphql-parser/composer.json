{
    "name": "graphql-by-pop/graphql-parser",
    "description": "GraphQL parser",
    "homepage": "https://github.com/GraphQLByPoP/graphql-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "symfony/property-access": "^2.8 || ^3.4 || ^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.3"
    },
    "autoload": {
        "psr-4": {
            "GraphQLByPoP\\GraphQLParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphQLByPoP\\GraphQLParser\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
