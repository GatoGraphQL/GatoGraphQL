{
    "name": "getpop/engine-wp-bootloader",
    "type": "wordpress-muplugin",
    "description": "PoP Engine Bootloader for WordPress",
    "keywords": ["pop", "engine", "WordPress", "bootloader"],
    "homepage": "https://github.com/getpop/engine-wp-bootloader",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/leoloso/wp-muplugin-loader.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "composer/installers": "~1.0",
        "getpop/routing-wp": "^0.7.8",
        "lkwdwrd/wp-muplugin-loader": "dev-feature-composer-v2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
