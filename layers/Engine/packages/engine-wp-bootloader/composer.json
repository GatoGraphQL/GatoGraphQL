{
    "name": "getpop/engine-wp-bootloader",
    "type": "wordpress-muplugin",
    "description": "PoP Engine Bootloader for WordPress",
    "keywords": ["Gato", "GraphQL", "PHP", "pop", "engine", "WordPress", "bootloader"],
    "homepage": "https://github.com/getpop/engine-wp-bootloader",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "boxuk/wp-muplugin-loader": "^1.6",
        "composer/installers": "~1.0",
        "getpop/root-wp": "^0.11"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "boxuk/wp-muplugin-loader": true,
            "composer/installers": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
