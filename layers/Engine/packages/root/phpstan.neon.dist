			message: '#Static property PoP\\Root\\Container\\SystemContainerBuilderFactory\:\:\$instance \(Symfony\\Component\\DependencyInjection\\Container\) does not accept SystemServiceContainer\.#'
			path: src/Container/SystemContainerBuilderFactory.php
			count: 1
