    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        # PHPStan bug!?
        -
            message: '#^Anonymous function should return (.*) but returns non-empty-string\.$#'
            path: src/ContentProcessors/AbstractContentParser.php
    # bootstrapFiles:
    #     - graphql-api.php
    level: 8
    paths:
        - src/
        - tests/
