{
    "name": "graphql-api/extension-demo",
    "autoload": {
        "psr-4": {
            "GraphQLAPI\\ExtensionDemo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GraphQLAPI\\ExtensionDemo\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "phpstan": "phpstan analyse -l 8 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "getpop/cache-control": "^0.9",
        "getpop/component-model": "^0.9",
        "getpop/definitions": "^0.9",
        "getpop/engine": "^0.9",
        "getpop/field-query": "^0.9",
        "getpop/guzzle-helpers": "^0.9",
        "getpop/loosecontracts": "^0.9",
        "getpop/mandatory-directives-by-configuration": "^0.9",
        "getpop/modulerouting": "^0.9",
        "getpop/query-parsing": "^0.9",
        "getpop/root": "^0.9",
        "pop-cms-schema/schema-commons": "^0.9",
        "pop-schema/schema-commons": "^0.9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
