{
    "name": "pop-schema/locations-wp-em",
    "type": "library",
    "description": "Implementation for WordPress of contracts from package \"Locations\"",
    "keywords": ["PoP", "locations-wp-em"],
    "homepage": "https://github.com/PoPSchema/locations-wp-em",
    "license": "GPLv2",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "pop-schema/locations": "^0.8",
        "getpop/engine-wp": "^0.8",
        "pop-schema/migrate-locations-wp-em": "^0.8"
    },
    "require-dev": {
        "wpackagist-plugin/events-manager": "^5.9",
        "phpunit/phpunit": ">=9.3",
        "rector/rector": "^0.9",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "suggest": {
        "wpackagist-plugin/events-manager": "This plugin is required"
    },
    "autoload": {
        "psr-4": {
            "PoPSchema\\LocationsWPEM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoPSchema\\LocationsWPEM\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
