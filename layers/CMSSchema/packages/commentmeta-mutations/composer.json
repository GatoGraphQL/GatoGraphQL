{
    "name": "pop-cms-schema/commentmeta-mutations",
    "type": "library",
    "description": "Adds support for comment mutations",
    "keywords": ["Gato", "GatoGraphQL", "GraphQL", "PHP", "commentmeta-mutations"],
    "homepage": "https://github.com/PoPCMSSchema/commentmeta-mutations",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://gatographql.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "pop-cms-schema/commentmeta": "^11.3",
        "pop-cms-schema/comment-mutations": "^11.3",
        "pop-cms-schema/custompost-mutations": "^11.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.18",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PoPCMSSchema\\CommentMetaMutations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoPCMSSchema\\CommentMetaMutations\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.3-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
