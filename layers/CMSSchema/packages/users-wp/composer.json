{
    "name": "pop-cms-schema/users-wp",
    "type": "library",
    "description": "Implementation for WordPress of contracts from package \"Users\"",
    "keywords": ["GraphQL", "PHP", "Gato", "GatoGraphQL", "users-wp"],
    "homepage": "https://github.com/PoPCMSSchema/users-wp",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "pop-cms-schema/queriedobject-wp": "^0.11",
        "pop-cms-schema/users": "^0.11"
    },
    "require-dev": {
        "johnpbloch/wordpress": ">=5.5",
        "phpunit/phpunit": "^9.5",
        "pop-cms-schema/customposts-wp": "^0.11",
        "rector/rector": "^0.17",
        "squizlabs/php_codesniffer": "^3.6",
        "szepeviktor/phpstan-wordpress": "^1.0"
    },
    "suggest": {
        "pop-cms-schema/customposts-wp": "Integration with Custom Posts for WordPress"
    },
    "autoload": {
        "psr-4": {
            "PoPCMSSchema\\UsersWP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoPCMSSchema\\UsersWP\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "wordpress-install-dir": "vendor/wordpress/wordpress",
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
