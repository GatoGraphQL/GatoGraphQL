{
    "name": "phpunit-for-gatographql/wpfaker-schema",
    "type": "library",
    "description": "Utilities to execute PHPUnit for Gato GraphQL",
    "keywords": ["Gato", "GatoGraphQL", "GraphQL", "PHP", "Gato", "GatoGraphQL", "wpfaker", "phpunit"],
    "homepage": "https://github.com/PHPUnitForGatoGraphQL/wpfaker-schema",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "getpop/engine": "^0.11",
        "leoloso/faker": "dev-master",
        "pop-backbone/wp-data-parser": "^0.11"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.17.8",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PHPUnitForGatoGraphQL\\WPFakerSchema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnitForGatoGraphQL\\WPFakerSchema\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
