{
    "name": "phpunit-for-gato-graphql/gato-graphql-testing",
    "type": "wordpress-plugin",
    "description": "Utilities for testing Gato GraphQL",
    "keywords": ["GraphQL", "PHP", "Gato", "GatoGraphQL", "WordPress", "GraphQL", "API"],
    "homepage": "https://github.com/PHPUnitForGatoGraphQL/gato-graphql-testing",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.15",
        "squizlabs/php_codesniffer": "^3.6",
        "szepeviktor/phpstan-wordpress": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PHPUnitForGatoGraphQL\\GatoGraphQLTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnitForGatoGraphQL\\GatoGraphQLTesting\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
