<{{tag}} class="btn-inner {{class}}" style="{{style}}">
	{{#if fontawesome}}<i class="fa {{fontawesome}}"></i>{{/if}}<span class="pop-btn-title {{classes.btn-title}}" style="{{styles.btn-title}}">{{{titles.btn}}}{{#if text-field}} {{{textfield-open}}}<span class="{{dbKey}}-{{text-field}}-{{dbObject.id}} {{classes.text-field}}" style="{{styles.text-field}}">{{{get dbObject text-field}}}</span>{{{textfield-close}}}{{/if}}</span>
</{{tag}}>