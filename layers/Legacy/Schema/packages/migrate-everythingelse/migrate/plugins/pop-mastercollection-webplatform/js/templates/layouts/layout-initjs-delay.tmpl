<div {{#generateId}}{{id}}{{/generateId}}></div>
<script type="text/javascript">
(function($){
	{{! Comment Leo 11/06/2017: do it inside "component:merged", so that it works also for server-side rendering (object pop.Manager is only available at the end)}}
	$(document).one('component:merged', function() {
		var block = pop.Manager.getBlock($('#{{bs.bId}}'));
		block.one("js:initialized", function() { 
			$("#{{lastGeneratedId}}").html('{{#withModule . "layout"}}{{enterModule ../.}}{{/withModule}}'); 
		});
	});
})(jQuery);
</script>