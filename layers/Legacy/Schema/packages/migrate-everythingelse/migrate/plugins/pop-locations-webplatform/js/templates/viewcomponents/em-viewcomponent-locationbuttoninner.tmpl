{{#with dbObject}}
	{{#if ../subcomponentoutputnames.location-layout}}
		{{#each locations}}
			{{#if @index}}{{../../separator}}{{/if}}
			{{#if ../../fontawesome}}
				<i class="fa {{../../fontawesome}}"></i>
			{{/if}}
			{{#withModule ../../. "location-layout"}}
				{{enterModule ../../../. typeOutputKey=../../../bs.outputKeys.locations objectID=../this}}
			{{/withModule}}
		{{/each}}
	{{else}}
		{{../titles.locations}}
	{{/if}}
{{/with}}