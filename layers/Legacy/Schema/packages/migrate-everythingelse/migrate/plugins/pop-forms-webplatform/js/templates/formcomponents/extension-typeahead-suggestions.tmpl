{{#if suggestions}}
	<div class="suggestions {{classes.suggestions}}" style="{{styles.suggestions}}">
		{{{titles.suggestions}}}
		{{#each suggestions}}
			<a {{#generateId context=../. group="suggestions"}}{{../id}}-{{this}}{{/generateId}} href="#" data-objectid="{{this}}" data-dbkey="{{../outputKeys.suggestions}}" data-target="#{{lastGeneratedId context=../.}}" class="{{../classes.suggestion}}" style="{{../styles.suggestion}}">
				{{#if ../suggestions-fontawesome}}
					<i class="fa {{../suggestions-fontawesome}}"></i>
				{{/if}}
				{{#withModule ../. "suggestions-layout"}}
					{{enterModule ../../. dbKey=../../outputKeys.suggestions objectID=../.}}
				{{/withModule}}
			</a>
		{{/each}}
	</div>
{{/if}}