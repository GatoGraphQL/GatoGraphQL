{
    "name": "pop-sites-wassup/notification-mutations",
    "type": "library",
    "description": "Adds mutations for notifications",
    "keywords": ["GraphQL", "PHP", "Gato", "GatoGraphQL", "notification-mutations"],
    "homepage": "https://github.com/PoPSites-Wassup/notification-mutations",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "pop-cms-schema/user-state": "^0.11"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.17",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PoPSitesWassup\\NotificationMutations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoPSitesWassup\\NotificationMutations\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
