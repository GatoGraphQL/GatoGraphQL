        "pop-schema/event-mutations-wp-em": "^0.8",
        "pop-schema/everythingelse-wp": "^0.8",
        "pop-schema/highlights-wp": "^0.8",
        "pop-schema/locationposts-wp": "^0.8",
        "pop-schema/stances-wp": "^0.8",
        "pop-sites-wassup/post-mutations": "^0.8",
        "pop-sites-wassup/highlight-mutations": "^0.8",
        "pop-sites-wassup/stance-mutations": "^0.8",
        "pop-sites-wassup/comment-mutations": "^0.8",
        "pop-sites-wassup/system-mutations": "^0.8",
        "pop-sites-wassup/gravityforms-mutations": "^0.8",
        "pop-sites-wassup/contactus-mutations": "^0.8",
        "pop-sites-wassup/contactuser-mutations": "^0.8",
        "pop-sites-wassup/newsletter-mutations": "^0.8",
