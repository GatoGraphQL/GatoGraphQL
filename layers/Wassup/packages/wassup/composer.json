        "pop-sites-wassup/locationpost-mutations": "^0.8",
        "pop-sites-wassup/postlink-mutations": "^0.8",
        "pop-sites-wassup/eventlink-mutations": "^0.8",
        "pop-sites-wassup/locationpostlink-mutations": "^0.8",
        "pop-sites-wassup/notification-mutations": "^0.8",
        "pop-sites-wassup/socialnetwork-mutations": "^0.8",
        "pop-sites-wassup/user-state-mutations": "^0.8",
        "pop-sites-wassup/everythingelse-mutations": "^0.8"
