    "keywords": ["PoP", "wassup"],
    "homepage": "https://github.com/PoPSites-Wassup/wassup",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "getpop/api-rest": "^0.8",
        "getpop/engine-wp-bootloader": "^0.8",
        "getpop/function-fields": "^0.8",
        "getpop/trace-tools": "^0.8",
        "getpop/site-wp": "^0.8",
        "getpop/spa": "^0.8",
